{
  "name": "riva",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "prettier:check": "prettier --config ./prettier.config.js --list-different \"projects/**/src/{app,lib,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "prettier:fix": "prettier --write \"projects/**/src/{app,lib,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "build:lib": "ng build riva-core && ng build riva-shared",
    "build:rivaportal": "ng build riva-portal --prod --aot -vc -cc --buildOptimizer --no-progress --output-hashing=all",
    "build:rivaadministrativeportal": "ng build riva-administrative-portal --prod --aot -vc -cc --buildOptimizer --no-progress --output-hashing=all"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.11",
    "@angular/cdk": "~10.1.3",
    "@angular/common": "~10.0.11",
    "@angular/compiler": "~10.0.11",
    "@angular/core": "~10.0.11",
    "@angular/forms": "~10.0.11",
    "@angular/localize": "~10.0.11",
    "@angular/platform-browser": "~10.0.11",
    "@angular/platform-browser-dynamic": "~10.0.11",
    "@angular/router": "~10.0.11",
    "@aspnet/signalr": "~1.1.4",
    "@ng-bootstrap/ng-bootstrap": "~7.0.0",
    "@ng-select/ng-select": "~5.0.8",
    "@ngx-translate/core": "~13.0.0",
    "@sweetalert2/ngx-sweetalert2": "~8.1.1",
    "angular-oauth2-oidc": "~10.0.3",
    "bootstrap": "~4.5.2",
    "core-js": "~3.6.5",
    "jquery": "~3.5.1",
    "ngx-device-detector": "~1.4.6",
    "ngx-perfect-scrollbar": "~10.0.1",
    "ngx-spinner": "~10.0.1",
    "ngx-toastr": "~13.1.0",
    "ngx-ui-switch": "~10.0.2",
    "node-sass": "~4.14.1",
    "popper.js": "~1.16.1",
    "prismjs": "~1.21.0",
    "rollup": "~2.22.0",
    "rxjs": "~6.6.2",
    "screenfull": "~5.0.2",
    "sweetalert2": "^10.8.1",
    "swiper": "~6.1.2",
    "tslib": "~2.0.1",
    "zone.js": "~0.10.3"
